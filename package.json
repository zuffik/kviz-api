{
  "name": "kvizapi",
  "version": "0.0.0",
  "description": "Node server written in TS.",
  "private": true,
  "scripts": {
    "start": "rm -rf dist && npm run build && node dist/server.js",
    "build": "tsc",
    "test": "exit 0"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "mjs"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "kafer-module": "git+https://gitlab.com/elias-it-solutions/kafer-module.git",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "tslint-react": "^3.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.2",
    "@types/lodash": "^4.14.63",
    "@types/mysql": "^2.15.5",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "concurrently": "^3.4.0",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.8",
    "tslint": "^5.11.0",
    "typescript": "^2.2.2"
  }
}
